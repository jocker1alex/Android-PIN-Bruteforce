PIN_TYPE=PIN
PIN_LENGTH=4

DELAY_BETWEEN_KEYS=0.2

PROGRESSIVE_ARRAY_ATTEMPT_COUNT__________=(1  11 40 50  60  70  80  90   100)
PROGRESSIVE_ARRAY_ATTEMPTS_UNTIL_COOLDOWN=(5  1  1  1   1   1   1   1    1)
PROGRESSIVE_ARRAY_COOLDOWN_IN_SECONDS____=(30 30 60 120 240 480 960 1920 3840)
##  10 pins ~ 1 min
##  40 pins ~ 1 + 15 = 16 min
##  50 pins ~ 1 + 15 + 10 = 26 min
##  60 pins ~ 1 + 15 + 10 + 20 = 46 min
##  70 pins ~ 1 + 15 + 10 + 20 + 40 = 86 min (01:26)
##  80 pins ~ 1 + 15 + 10 + 20 + 40 + 80 = 166 min (02:46)
##  90 pins ~ 1 + 15 + 10 + 20 + 40 + 80 + 160 = 326 min (05:26)
## 100 pins ~ 1 + 15 + 10 + 20 + 40 + 80 + 160 + 320 = 646 min (10:46)

SEND_KEYS_DISMISS_POPUPS_N_SECONDS_BEFORE_COOLDOWN_END=1
SEND_KEYS_DISMISS_POPUPS_AT_COOLDOWN_END="escape"

KEYS_BEFORE_EACH_PIN="space"
KEYS_AFTER_EACH_PIN="tab tab tab tab tab"

DELAY_AFTER_KEYS_BEFORE_EACH_PIN=0.2

KEYS_STAY_AWAKE_DURING_COOLDOWN=""

SEND_KEYS_STAY_AWAKE_DURING_COOLDOWN_EVERY_N_SECONDS=0

DELAY_BEFORE_STARTING=1
KEYS_BEFORE_STARTING="space"

RESUME_FROM_PIN=

LOG=bruter.log

EXIT_AFTER_FAIL_COUNT=15

KEYBOARD_DEVICE=/dev/hidg0
HID_KEYBOARD=/system/xbin/hid-keyboard
USB_DEVICES=/usr/bin/usb-devices
